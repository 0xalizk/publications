	keywords = {complex network, HOT, Internet topology, network design, scale-free network},
	pages = {14497--14502},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/UGZSXKD3/14497.html:text/html}
