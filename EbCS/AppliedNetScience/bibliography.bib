@article{west_fourth_1999-1,
	title = {The {Fourth} {Dimension} of {Life}: {Fractal} {Geometry} and {Allometric} {Scaling} of {Organisms}},
	volume = {284},
	issn = {0036-8075, 1095-9203},
	shorttitle = {The {Fourth} {Dimension} of {Life}},
	url = {http://science.sciencemag.org/content/284/5420/1677},
	doi = {10.1126/science.284.5420.1677},
	abstract = {Fractal-like networks effectively endow life with an additional fourth spatial dimension. This is the origin of quarter-power scaling that is so pervasive in biology. Organisms have evolved hierarchical branching networks that terminate in size-invariant units, such as capillaries, leaves, mitochondria, and oxidase molecules. Natural selection has tended to maximize both metabolic capacity, by maximizing the scaling of exchange surface areas, and internal efficiency, by minimizing the scaling of transport distances and times. These design principles are independent of detailed dynamics and explicit models and should apply to virtually all organisms.},
	language = {en},
	number = {5420},
	urldate = {2017-02-27},
	journal = {Science},
	author = {West, Geoffrey B. and Brown, James H. and Enquist, Brian J.},
	month = jun,
	year = {1999},
	pmid = {10356399},
	pages = {1677--1679},
	annote = {Recently, we presented a model which suggested that the explanation could be found in the fractal-like architecture of the hierarchical branching vascular networks that distribute resources within organisms (2).
Biological scaling can be described by the allometric equationY = Y 0 Mb , where Y is a variable such as metabolic rate or life span, Y 0 is a normalization constant, and b is a scaling exponent (1). Whereas Y 0 varies with the trait and type of organism, b characteristically takes on a limited number of values, all of which are simple multiples of 1/4. For example, diameters of tree trunks and aortas scale asM 3/8 rates of cellular metabolism and heartbeat as M −1/4, blood circulation time and life span as M 1/4, and whole-organism metabolic rate as M 3/4. The question has been why these exponents are multiples of 1/4 rather than 1/3 as expected on the basis of conventional Euclidean geometric scaling.},
	file = {Full Text PDF:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/6J75SQ2C/West et al. - 1999 - The Fourth Dimension of Life Fractal Geometry and.pdf:application/pdf;Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/DNU2XQZU/1677.html:text/html}
}

