@article{cubitt_undecidability_2015-1,
	title = {Undecidability of the spectral gap},
	volume = {528},
	copyright = {© 2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
	url = {http://www.nature.com/nature/journal/v528/n7581/abs/nature16059.html},
	doi = {10.1038/nature16059},
	abstract = {The spectral gap—the energy difference between the ground state and first excited state of a system—is central to quantum many-body physics. Many challenging open problems, such as the Haldane conjecture, the question of the existence of gapped topological spin liquid phases, and the Yang–Mills gap conjecture, concern spectral gaps. These and other problems are particular cases of the general spectral gap problem: given the Hamiltonian of a quantum many-body system, is it gapped or gapless? Here we prove that this is an undecidable problem. Specifically, we construct families of quantum spin systems on a two-dimensional lattice with translationally invariant, nearest-neighbour interactions, for which the spectral gap problem is undecidable. This result extends to undecidability of other low-energy properties, such as the existence of algebraically decaying ground-state correlations. The proof combines Hamiltonian complexity techniques with aperiodic tilings, to construct a Hamiltonian whose ground state encodes the evolution of a quantum phase-estimation algorithm followed by a universal Turing machine. The spectral gap depends on the outcome of the corresponding ‘halting problem’. Our result implies that there exists no algorithm to determine whether an arbitrary model is gapped or gapless, and that there exist models for which the presence or absence of a spectral gap is independent of the axioms of mathematics. View full text},
	language = {en},
	number = {7581},
	urldate = {2017-06-20},
	journal = {Nature},
	author = {Cubitt, Toby S. and Perez-Garcia, David and Wolf, Michael M.},
	month = dec,
	year = {2015},
	note = {bibtex: cubitt\_undecidability\_2015},
	keywords = {Quantum information, Theoretical physics},
	pages = {207--211},
	annote = {see https://www.youtube.com/watch?v=8bLXHvH9s1A at minute 50 (and listen carefully at minute 52, how a statement from complexity theory can make a prediction about physics)   and Ctrl-F "undecidability of spectral gap" here: http://www.scottaaronson.com/blog/?p=2586},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/889GUPUR/nature16059.html:text/html}
}

