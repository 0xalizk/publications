	keywords = {Bacterial genetics, Evolutionary genetics, Experimental evolution},
	pages = {165--170},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/HDPDV89I/nature18959.html:text/html}
