	month = dec,
	year = {2011},
	keywords = {Algorithms, Area Under Curve, Computational Biology, Computer Simulation, Databases, Genetic, Gene Expression Profiling, Gene Regulatory Networks, Kinetics, Models, Genetic, Oligonucleotide Array Sequence Analysis},
