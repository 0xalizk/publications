@article{noauthor_dominating_2015-1,
	title = {Dominating set},
	copyright = {Creative Commons Attribution-ShareAlike License},
	url = {http://en.wikipedia.org/w/index.php?title=Dominating_set&oldid=652697966},
	abstract = {In graph theory, a dominating set for a graph G = (V, E) is a subset D of V such that every vertex not in D is adjacent to at least one member of D. The domination number γ(G) is the number of vertices in a smallest dominating set for G. The dominating set problem concerns testing whether γ(G) ≤ K for a given graph G and input K; it is a classical NP-complete decision problem in computational complexity theory (Garey \& Johnson 1979). Therefore it is believed that there is no efficient algorithm that finds a smallest dominating set for a given graph. Figures (a)–(c) on the right show three examples of dominating sets for a graph. In each example, each white vertex is adjacent to at least one red vertex, and it is said that the white vertex is dominated by the red vertex. The domination number of this graph is 2: the examples (b) and (c) show that there is a dominating set with 2 vertices, and it can be checked that there is no dominating set with only 1 vertex for this graph.},
	language = {en},
	urldate = {2015-05-28},
	journal = {Wikipedia, the free encyclopedia},
	month = mar,
