	year = {2010},
	note = {bibtex: corne\_optimisation\_2010},
	keywords = {Algorithm Analysis and Problem Complexity, Artificial Intelligence (incl. Robotics), Computational Biology/Bioinformatics, Computation by Abstract Devices, Discrete Mathematics in Computer Science, Pattern Recognition},
	pages = {22--31}
}

