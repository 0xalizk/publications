	note = {bibtex: reynaldo\_kinetics\_2000},
	keywords = {Base Pairing, Binding Sites, Chemical, Complementary, DNA, DNA Probes, Fluorescent Dyes, Kinetics, Models, Molecular Weight, Nucleic Acid Denaturation, Nucleic Acid Hybridization, Oligodeoxyribonucleotides, Osmolar Concentration, Temperature, Thermodynamics},
	pages = {511--520},
	annote = {Referenced in http://www.nature.com/nchem/journal/v3/n2/box/nchem.957\_BX1.html similar references: 21, 22, 23, 24, 25, 26, 27, 28  in the same article}
}

