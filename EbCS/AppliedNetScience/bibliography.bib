@article{parks_polymerase_1991,
	title = {A polymerase chain reaction mediated by a single primer: cloning of genomic sequences adjacent to a serotonin receptor protein coding region.},
	volume = {19},
	issn = {0305-1048},
	shorttitle = {A polymerase chain reaction mediated by a single primer},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC332551/},
	abstract = {Under appropriate conditions, specific double-stranded DNA product was generated after amplification of genomic DNA sequences in a polymerase chain-like reaction that contained only a single primer. This type of amplification reaction was performed with a variety of primers and substrate DNAs. In addition to nonspecific heterogeneous products, 5 of 11 primers reproducibly directed synthesis of double-stranded DNA that corresponded to the region of the template that contained the authentic primer annealing site. Three of these amplified products were cloned and their ends were sequenced. All three contained a copy of the primer at both 5' ends, and the position of one of the primers represented the authentic primer binding site. In each case, the location of the second copy of the primer indicated that it had initially hybridized to a partially homologous sequence in the template DNA. This single primer reaction makes it possible to amplify and clone a DNA region of unknown sequence that is adjacent to a known DNA sequence. One of the single primer reaction products described here included sequence to the 5' side of the coding region of a serotonin receptor gene that contained a functional promoter.},
	number = {25},
	urldate = {2016-12-03},
	journal = {Nucleic Acids Research},
	author = {Parks, C L and Chang, L S and Shenk, T},
	month = dec,
	year = {1991},
	pmid = {1766875},
	pmcid = {PMC332551},
	pages = {7155--7160},
	annote = {"single primer" by virtue of random sequence appearing in the two strands},
	file = {PubMed Central Full Text PDF:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/XTPTTTZ9/Parks et al. - 1991 - A polymerase chain reaction mediated by a single p.pdf:application/pdf}
}

@article{green_toehold_2014,
	title = {Toehold {Switches}: {De}-{Novo}-{Designed} {Regulators} of {Gene} {Expression}},
	volume = {159},
	issn = {0092-8674},
	shorttitle = {Toehold {Switches}},
	url = {http://www.sciencedirect.com/science/article/pii/S0092867414012896},
	doi = {10.1016/j.cell.2014.10.002},
	abstract = {Summary
Efforts to construct synthetic networks in living cells have been hindered by the limited number of regulatory components that provide wide dynamic range and low crosstalk. Here, we report a class of de-novo-designed prokaryotic riboregulators called toehold switches that activate gene expression in response to cognate RNAs with arbitrary sequences. Toehold switches provide a high level of orthogonality and can be forward engineered to provide average dynamic range above 400. We show that switches can be integrated into the genome to regulate endogenous genes and use them as sensors that respond to endogenous RNAs. We exploit the orthogonality of toehold switches to regulate 12 genes independently and to construct a genetic circuit that evaluates 4-input AND logic. Toehold switches, with their wide dynamic range, orthogonality, and programmability, represent a versatile and powerful platform for regulation of translation, offering diverse applications in molecular biology, synthetic biology, and biotechnology.},
	number = {4},
	urldate = {2017-04-27},
	journal = {Cell},
	author = {Green, Alexander A. and Silver, Pamela A. and Collins, James J. and Yin, Peng},
	month = nov,
	year = {2014},
	pages = {925--939},
	file = {ScienceDirect Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/A9MT6AA8/S0092867414012896.html:text/html}
}

