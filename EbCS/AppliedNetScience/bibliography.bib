	keywords = {Gene Regulatory Networks, Genetic Association Studies, Genetic Predisposition to Disease, Genotype, Humans, Metabolic Networks and Pathways, Models, Genetic, Mutation, Phenotype, Protein Interaction Domains and Motifs},
