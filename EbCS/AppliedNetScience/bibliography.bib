	month = sep,
	year = {2016},
	pmid = {27708008},
	note = {bibtex: costanzo\_global\_2016 },
	pages = {aaf1420},
	annote = {asymmetrical promotional (positive) and inhibitory (negative repressive) signs},
	annote = {The following fields were not imported:annote="Negative interactions connected functionally related genes, mapped core bioprocesses, and identified pleiotropic genes, whereas positive interactions often mapped general regulatory connections associated with defects in cell cycle progression or cellular proteostasis. Importantly, the global network illustrates how coherent sets of negative or positive genetic interactions connect protein complex and pathways to map a functional wiring diagram of the cell."   "Negative genetic interactions tend to connect functionally related genes and thus may be predicted using alternative functional information."},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/VZ65B5R4/aaf1420.html:text/html}
