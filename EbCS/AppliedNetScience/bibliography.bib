	note = {bibtex: mehta\_complexity\_2014-1},
	keywords = {Computer Science - Computational Complexity, Computer Science - Computer Science and Game Theory, Mathematics - Dynamical Systems, Mathematics - Spectral Theory, Quantitative Biology - Populations and Evolution},
	annote = {Comment: 24 pages, 2 figues}
}

