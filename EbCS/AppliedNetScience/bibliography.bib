}

@article{friston_consciousness_nodate-1,
	title = {Consciousness is not a thing, but a process of inference – {Karl} {Friston} textbar {Aeon} {Essays}},
	url = {https://aeon.co/essays/consciousness-is-not-a-thing-but-a-process-of-inference},
	abstract = {The special trick of consciousness is being able to project action and time into a range of possible futures},
	urldate = {2017-06-19},
	journal = {Aeon},
	author = {Friston, Karl},
	note = {bibtex: friston\_consciousness\_nodate},
	annote = {From the comment section:   Toosoon Executus "These articles are always so frustrating. By this logic, if I spend 2 hours coding up a little computer program that can take in some sensory data and do some basic prediction, my computer program is conscious? The reason no one takes new theories about consciousness seriously (this one will be included, just like the 20 others published in the last year) is because they all claim to have “solved” the problem, or at least come up with a “novel” way of looking at the problem, without answering ANY of the hugely difficult mysteries about the true nature of consciousness that anyone in the field ACTUALLY cares about. I’m not going to spell out all of the questions that need to be answered, there are too many. But you can’t just say “it’s inference!” and think that you’ve contributed anything meaningful to the discussion. The REAL problems (unity, qualia, correlation to biology, causal/non-causal, and dozens more huge mysteries) are never even remotely touched by any of these new theories. Not by Graziano, not by Anil Seth, certainly not by Sean Carroll (it’s a ‘way of talking’?? really??), and not by this theory. This is all stuff that an undergraduate psychology or biology major could come up with, but they don’t write essays about it because they are at least capable understanding their own ignorance on the subject. Stop giving us theories and start giving us **explanations**. Call it whatever you want! Inference, stories, attractors, complexity, illusion, microtubules, wave function collapse, fairy magic. If your explanation is good, you will be able to **build** a conscious being because your theory works. None of these work (as explanations of consciousness). Incidentally, neither does the current paradigm of machine learning for many of the same reasons - it’s not an explanation, it’s an application of statistics programming."},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/ASBX2J4G/consciousness-is-not-a-thing-but-a-process-of-inference.html:text/html}
}

@article{gerstein_architecture_2012-5,
