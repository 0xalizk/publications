	pages = {370--374},
	file = {PubMed Central Full Text PDF:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/MQQ8WHAU/Carvunis et al. - 2012 - Proto-genes and de novo gene birth.pdf:application/pdf}
