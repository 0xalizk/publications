	number = {6093},
	urldate = {2017-06-14},
	journal = {Science},
	author = {Jarrell, Travis A. and Wang, Yi and Bloniarz, Adam E. and Brittin, Christopher A. and Xu, Meng and Thomson, J. Nichol and Albertson, Donna G. and Hall, David H. and Emmons, Scott W.},
	month = jul,
	year = {2012},
	pmid = {22837521},
	pages = {437--444},
	file = {Snapshot:/Users/mohammedalshamrani/Library/Application Support/Firefox/Profiles/iqvrtqao.default/zotero/storage/9ZPT3JCK/437.html:text/html}
